{
  "translate": {
    "name": "Translation",
    "label": "Compatibility",
    "author": "Flycart",
    "version": "1.0.0",
    "description": "The add-on helps you translate Discount Rules using plugins WPML and Loco Translate.",
    "icon_url": "https://raw.githubusercontent.com/flycartinc/wdr-addons/v3/icons-v3/wdr-translate.png",
    "addon_url": "https://www.flycart.org/discount-rules-add-ons ",
    "download_url": "",
    "plugin_file": "wdr-translate/wdr-translate.php",
    "page_url": "{addon_page}",
    "settings_url": "",
    "requires": {
      "wp": "5.3",
      "wc": "4.4",
      "wdr_core": "3.0",
      "wdr_pro": "3.0"
    },
    "dependencies": {
      "wpml": {
        "name": "WPML",
        "url": "https://wpml.org",
        "file": "sitepress-multilingual-cms/sitepress.php"
      },
      "logo_translate": {
        "name": "Loco Translate",
        "url": "https://wordpress.org/plugins/loco-translate",
        "file": "loco-translate/loco.php"
      }
    },
    "is_pro": false
  },
